{
  "name": "trig-calculator",
  "version": "1.0.0",
  "description": "A web app to calculate triangle angles and sides",
  "scripts": {
    "build": "webpack --env=prod --progress --profile --colors",
    "start": "webpack-dev-server --env=dev &>/dev/null &",
    "stop": "pkill -f webpack-dev-server || true",
    "test": "jest src/.*.test.jsx?",
    "lint": "eslint .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://epeterson320@bitbucket.org/epeterson320/trig-calculator.git"
  },
  "keywords": [
    "Trigonometry",
    "Calculator",
    "Triangle",
    "Trig"
  ],
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "author": "Eric Peterson",
  "license": "MIT",
  "homepage": "https://bitbucket.org/epeterson320/trig-calculator#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.8",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "gh-pages": "^0.12.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "nightwatch": "^0.9.14",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^2.6.1"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(scss)$": "identity-obj-proxy"
    }
  }
}
